<?xml version='1.0' encoding='utf-8'?>
<widget id="com.esaybooker.qr" version="1.3.11" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <feature name="QRScanner">
        <param name="browser-package" value="QRScanner" />
    </feature>
    <name>Easybooker QR-scanner</name>
    <description>
        Easybooker QR-scanner
    </description>
    <author email="bizz@easybooker.be" href="http://easybooker.be">
        Easybooker
    </author>
    <content src="index.html" />
    <allow-navigation href="*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-navigation href="https://napp.easybooker.be/*" />
    <allow-navigation href="*://*.easybooker.be/*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-navigation href="http://*/*" />
    <allow-navigation href="https://*/*" />
    <access origin="*" />
    <access origin="https://easybooker.be" />
    <access origin="https://napp.easybooker.be" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="android-build-tool" value="gradle" />
</widget>
